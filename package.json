  "bugs": {
    "url": "https://github.com/PravinKumar95/Portfolio/issues"
  },
  "homepage": "https://github.com/PravinKumar95/Portfolio#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-react-app": "^3.1.2"
  },
  "devDependencies": {
    "http-server": "^14.1.0",
    "nodemon": "^2.0.16",
    "npm-run-all": "^4.1.5"
  }
}
