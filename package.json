{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "My portfolio site",
  "main": "index.js",
  "scripts": {
    "start": "run-p babel:watch sass:watch",
    "serve": "http-server -p 8080",
    "serve:watch": "nodemon -e 'js,css,html' -x npm run serve",
    "babel:watch": "npx babel --watch src --out-dir public/js --presets react-app/prod",
    "sass:watch": "sass --watch src/index.scss public/css/bundle.css",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PravinKumar95/Portfolio.git"
  },
  "keywords": [],
